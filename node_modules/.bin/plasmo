#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/revan/code/extensions/ghw-workshop/node_modules/.pnpm/plasmo@0.53.0_t3zwlxai2rto6jslfmw3pdgiue/node_modules/plasmo/dist/node_modules:/home/revan/code/extensions/ghw-workshop/node_modules/.pnpm/plasmo@0.53.0_t3zwlxai2rto6jslfmw3pdgiue/node_modules/plasmo/node_modules:/home/revan/code/extensions/ghw-workshop/node_modules/.pnpm/plasmo@0.53.0_t3zwlxai2rto6jslfmw3pdgiue/node_modules:/home/revan/code/extensions/ghw-workshop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/revan/code/extensions/ghw-workshop/node_modules/.pnpm/plasmo@0.53.0_t3zwlxai2rto6jslfmw3pdgiue/node_modules/plasmo/dist/node_modules:/home/revan/code/extensions/ghw-workshop/node_modules/.pnpm/plasmo@0.53.0_t3zwlxai2rto6jslfmw3pdgiue/node_modules/plasmo/node_modules:/home/revan/code/extensions/ghw-workshop/node_modules/.pnpm/plasmo@0.53.0_t3zwlxai2rto6jslfmw3pdgiue/node_modules:/home/revan/code/extensions/ghw-workshop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../plasmo/dist/index.js" "$@"
else
  exec node  "$basedir/../plasmo/dist/index.js" "$@"
fi
