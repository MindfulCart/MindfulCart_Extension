#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/revan/code/git-projects/plasmo/mindful-cart/node_modules/.pnpm/plasmo@0.67.4_gjpyg5imi5mt2gbvkmn3a4dr7m/node_modules/plasmo/bin/node_modules:/home/revan/code/git-projects/plasmo/mindful-cart/node_modules/.pnpm/plasmo@0.67.4_gjpyg5imi5mt2gbvkmn3a4dr7m/node_modules/plasmo/node_modules:/home/revan/code/git-projects/plasmo/mindful-cart/node_modules/.pnpm/plasmo@0.67.4_gjpyg5imi5mt2gbvkmn3a4dr7m/node_modules:/home/revan/code/git-projects/plasmo/mindful-cart/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/revan/code/git-projects/plasmo/mindful-cart/node_modules/.pnpm/plasmo@0.67.4_gjpyg5imi5mt2gbvkmn3a4dr7m/node_modules/plasmo/bin/node_modules:/home/revan/code/git-projects/plasmo/mindful-cart/node_modules/.pnpm/plasmo@0.67.4_gjpyg5imi5mt2gbvkmn3a4dr7m/node_modules/plasmo/node_modules:/home/revan/code/git-projects/plasmo/mindful-cart/node_modules/.pnpm/plasmo@0.67.4_gjpyg5imi5mt2gbvkmn3a4dr7m/node_modules:/home/revan/code/git-projects/plasmo/mindful-cart/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../plasmo/bin/index.mjs" "$@"
else
  exec node  "$basedir/../plasmo/bin/index.mjs" "$@"
fi
